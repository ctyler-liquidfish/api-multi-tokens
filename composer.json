{
    "name": "ctyler-liquidfish/api-multi-tokens",
    "description": "Laravel API token guard with multiple tokens. Based off mayoz/laravel-tokens.",
    "keywords": ["laravel", "auth", "multiple token"],
    "license": "MIT",
    "support": {
        "source": "https://github.com/ctyler-larafish/api-multi-tokens"
    },
    "authors": [
        {
            "name": "J. Chris Tyler",
            "email": "ctyler@liquidfish.com"
        },
        {
            "name": "Sercan Cakir (original)",
            "email": "srcnckr@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "illuminate/auth": "~5.5",
        "illuminate/contracts": "~5.5",
        "illuminate/http": "~5.5",
        "illuminate/queue": "~5.5",
        "illuminate/support": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Liquidfish\\ApiMultiToken\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Liquidfish\\ApiMultiToken\\TokenServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
